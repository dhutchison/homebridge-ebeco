{
    "scenarioName": "Update Device State",
    "requiredScenarioState": "temperature updated to 24",
    "request": {
        "method": "GET",
        "url": "/api/services/app/Devices/GetUserDevices",
        "headers": {
            "Abp.TenantId": {
                "equalTo": "1"
            },
            "Content-Type": {
                "contains": "application/json"
